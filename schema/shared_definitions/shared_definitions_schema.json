{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$defs": {
    "color": {
      "type": "string",
      "customTypePath": "jsondoc.models.shared_definitions.Color",
      "enum": [
        "blue",
        "blue_background",
        "brown",
        "brown_background",
        "default",
        "gray",
        "gray_background",
        "green",
        "green_background",
        "orange",
        "orange_background",
        "yellow",
        "pink",
        "pink_background",
        "purple",
        "purple_background",
        "red",
        "red_background",
        "yellow_background"
      ]
    },
    "annotations": {
      "type": "object",
      "customTypePath": "jsondoc.models.shared_definitions.Annotations",
      "properties": {
        "bold": {
          "type": "boolean",
          "default": false
        },
        "italic": {
          "type": "boolean",
          "default": false
        },
        "strikethrough": {
          "type": "boolean",
          "default": false
        },
        "underline": {
          "type": "boolean",
          "default": false
        },
        "code": {
          "type": "boolean",
          "default": false
        },
        "color": {
          "type": "string",
          "default": "default"
          // TODO: Fix resolution of this reference
          // "$ref": "/shared_definitions/shared_definitions_schema.json#/$defs/color"
        }
      }
      // notion-diverge
      // "required": [
      //   "bold",
      //   "italic",
      //   "strikethrough",
      //   "underline",
      //   "code",
      //   "color"
      // ]
    }
  }
}
