{
  "name": "jsondoc",
  "version": "0.1.0",
  "description": "JSON-DOC TypeScript implementation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsup",
    "dev": "tsup --watch",
    "build:vite": "vite build",
    "dev:example": "vite --config vite.dev.config.ts",
    "preview": "vite preview --config vite.dev.config.ts",
    "generate-types": "ts-node scripts/generate-types.ts",
    "test": "jest",
    "view": "node scripts/viewer.js",
    "screenshot": "node scripts/screenshot.js",
    "prepublishOnly": "npm run clean && npm run generate-types && npm run build",
    "format": "prettier --write .",
    "check": "tsc --noEmit"
  },
  "keywords": [
    "json",
    "document",
    "notion",
    "schema"
  ],
  "author": "TextCortex Dev Team <dev@textcortex.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.27",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.0",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "json-schema-to-typescript": "^15.0.4",
    "prettier": "3.5.3",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "json5": "^2.2.3",
    "katex": "^0.16.22",
    "puppeteer": "^24.9.0",
    "strip-json-comments": "^5.0.2"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textcortex/JSON-DOC.git"
  },
  "bugs": {
    "url": "https://github.com/textcortex/JSON-DOC/issues"
  },
  "homepage": "https://github.com/textcortex/JSON-DOC#readme"
}
